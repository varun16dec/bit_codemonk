#include <iostream>
using namespace std;

int main()
{
    int t;
    scanf("%d",&t);
    while(t--){
        long n;
        scanf("%ld",&n);
        int count=0;
        for(int a=1 ;a<n ;a++){
            if((a^n)==a+n)
                count++;
        }
        printf("%d\n",count);
    }
    return 0;
}
